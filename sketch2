var X = 0;

background(255);
while (X <= 340) {
  {
fill(255);
  stroke(0);
  ellipse(X,100,30);
  X= X+40;
  background(255);
  }
}

var L = 40;
while (X > 0)
{
  {fill(255);
  stroke(0);
  ellipse(X,100,30); 
   X=X-L
  background(255);}
  
  }
  
  while (true)
  {
fill(255);
  stroke(0);
  ellipse(X,100,30);
  X= X+40;
  background(255);
  
  
  }
